{
  "name": "remix-validated-form",
  "version": "2.0.0-beta.0",
  "description": "Form component and utils for easy form validation in remix",
  "browser": "./browser/index.js",
  "main": "./build/index.js",
  "repository": {
    "type": "git",
    "url": "https://github.com/airjp73/remix-validated-form"
  },
  "sideEffects": false,
  "scripts": {
    "dev": "tsc --module ESNext --outDir ./browser --watch",
    "build": "npm run build:browser && npm run build:main",
    "build:browser": "tsc --module ESNext --outDir ./browser",
    "build:main": "tsc --module CommonJS --outDir ./build",
    "test": "jest src",
    "test:watch": "jest src --watch",
    "prepare": "husky install",
    "prepublishOnly": "npm run build:browser && npm run build:main"
  },
  "author": {
    "name": "Aaron Pettengill",
    "email": "pettengill.aaron@gmail.com",
    "url": "https://github.com/airjp73"
  },
  "license": "MIT",
  "keywords": [
    "remix",
    "remix.run",
    "react",
    "form",
    "yup",
    "validation"
  ],
  "peerDependencies": {
    "@remix-run/react": "^1.0.6",
    "@remix-run/server-runtime": "^1.0.6",
    "react": "^17.0.2"
  },
  "devDependencies": {
    "@remix-run/react": "^1.0.6",
    "@remix-run/server-runtime": "^1.0.6",
    "@types/jest": "^27.0.3",
    "@types/lodash": "^4.14.178",
    "@types/react": "^17.0.37",
    "fetch-blob": "^3.1.3",
    "react": "^17.0.2",
    "tsconfig": "*",
    "ts-jest": "^27.1.1",
    "typescript": "^4.5.3",
    "yup": "^0.32.11",
    "zod": "^3.11.6"
  },
  "dependencies": {
    "lodash": "^4.17.21",
    "tiny-invariant": "^1.2.0"
  }
}
