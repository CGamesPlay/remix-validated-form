{
  "name": "remix-validated-form",
  "version": "4.3.0",
  "description": "Form component and utils for easy form validation in remix",
  "browser": "./dist/remix-validated-form.cjs.js",
  "main": "./dist/remix-validated-form.cjs.js",
  "types": "./dist/types/index.d.ts",
  "repository": {
    "type": "git",
    "url": "https://github.com/airjp73/remix-validated-form"
  },
  "sideEffects": false,
  "scripts": {
    "dev": "tsc --module ESNext --outDir ./browser --watch",
    "build": "npm run build:vite",
    "build:browser": "tsc --module ESNext --outDir ./browser",
    "build:main": "tsc --module CommonJS --outDir ./build",
    "build:vite": "vite build",
    "prepublishOnly": "cp ../../README.md ./README.md && npm run build",
    "postpublish": "rm ./README.md"
  },
  "author": {
    "name": "Aaron Pettengill",
    "email": "pettengill.aaron@gmail.com",
    "url": "https://github.com/airjp73"
  },
  "license": "MIT",
  "keywords": [
    "remix",
    "remix.run",
    "react",
    "form",
    "yup",
    "zod",
    "validation"
  ],
  "peerDependencies": {
    "@remix-run/react": "1.x",
    "@remix-run/server-runtime": "1.x",
    "react": "^17.0.2 || ^18.0.0"
  },
  "devDependencies": {
    "@remix-run/react": "^1.2.1",
    "@remix-run/server-runtime": "^1.2.1",
    "@types/lodash": "^4.14.178",
    "@types/react": "^17.0.37",
    "fetch-blob": "^3.1.3",
    "react": "^17.0.2",
    "tsconfig": "*",
    "typescript": "^4.5.3",
    "vite": "^2.9.5",
    "vite-plugin-dts": "^1.1.0"
  },
  "dependencies": {
    "jotai": "^1.5.3",
    "lodash": "^4.17.21",
    "tiny-invariant": "^1.2.0"
  }
}
